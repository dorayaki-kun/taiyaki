{
  "name": "taiyaki",
  "description": "the lightweight library of WebGL.",
  "version": "0.5.12",
  "author": "dorayakikun",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.0",
    "babel-plugin-istanbul": "4.1.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babelify": "^8.0.0",
    "browserify": "^15.1.0",
    "eslint": "4.16.0",
    "eslint-config-precure": "^2.0.0",
    "flow-bin": "0.64.0",
    "flow-copy-source": "^1.2.1",
    "flow-typed": "^2.1.2",
    "husky": "^0.14.3",
    "jasmine-core": "^2.6.2",
    "karma": "^2.0.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-browserify": "5.1.3",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "lint-staged": "^6.0.0",
    "prettier": "1.10.0"
  },
  "files": [
    "README.md",
    "LICENSE",
    "index.js",
    "package.json",
    "src",
    "build"
  ],
  "keywords": [
    "WebGL"
  ],
  "license": "MIT",
  "lint-staged": {
    ".{js,json,md}": [
      "fmt",
      "git add"
    ]
  },
  "main": "build/src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dorayakikun/taiyaki.git"
  },
  "scripts": {
    "build": "npm run build:js && npm run build:flow",
    "build:js": "babel src --out-dir build/src",
    "build:flow": "flow-copy-source -v  src build/src",
    "clean": "rm -rf build/src*",
    "flow": "flow",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "karma": "karma start karma.conf.js --single-run",
    "precommit": "lint-staged",
    "prepublish": "precommit | npm run build",
    "prepush": "precommit | npm run karma",
    "test": "echo \"Please run karma\""
  }
}
