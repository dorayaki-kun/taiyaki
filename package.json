{
  "name": "taiyaki",
  "description": "the lightweight library of WebGL.",
  "version": "0.5.13",
  "author": "dorayakikun",
  "devDependencies": {
    "husky": "^0.14.3",
    "jasmine-core": "^3.0.0",
    "karma": "^2.0.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-browserify": "5.2.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "lint-staged": "^7.0.0",
    "prettier": "^1.10.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "typings": "^2.1.1"
  },
  "files": [
    "README.md",
    "LICENSE",
    "index.js",
    "package.json",
    "src",
    "build"
  ],
  "keywords": [
    "WebGL"
  ],
  "license": "MIT",
  "lint-staged": {
    ".{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dorayakikun/taiyaki.git"
  },
  "scripts": {
    "build": "babel src --out-dir build/src",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "flow": "flow",
    "karma": "karma start karma.conf.js --single-run",
    "lint": "eslint src",
    "precommit": "lint-staged",
    "prepare": "npm run build",
    "prepush": "npm run build",
    "test": "echo \"Please run karma\""
  }
}
